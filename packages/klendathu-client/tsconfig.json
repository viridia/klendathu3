{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "types": [
      "webpack-env"
    ]
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx"
  ]
}
